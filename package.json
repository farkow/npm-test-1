        "clsx": "1.2.1",
        "lodash": "4.17.21",
        "office-ui-fabric-react": "7.185.7",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-sortable-tree": "2.8.0",
        "tslib": "2.3.1"
    },
    "devDependencies": {
        "@emotion/eslint-plugin": "11.10.0",
        "@microsoft/eslint-config-spfx": "1.15.2",
        "@microsoft/eslint-plugin-spfx": "1.15.2",
