{
    "name": "npm-test-1",
    "version": "2.2.2",
    "private": true,
    "main": "lib/index.js",
    "scripts": {
        "build": "gulp bundle",
        "clean": "gulp clean",
        "test": "gulp test",
        "package:test": "gulp clean && gulp bundle && gulp package-solution",
        "package:test:without-mobile": "gulp clean && gulp build --remove-wiki-mobile && gulp bundle && gulp package-solution",
        "package:test:only-mobile": "gulp clean && gulp build --only-wiki-mobile && gulp bundle && gulp package-solution",
        "package:prod": "gulp clean --ship && gulp bundle --ship && gulp package-solution --ship",
        "package:prod:without-mobile": "gulp clean --ship && gulp build --remove-wiki-mobile && gulp bundle --ship && gulp package-solution --ship",
        "package:prod:only-mobile": "gulp clean --ship && gulp build --only-wiki-mobile && gulp bundle --ship && gulp package-solution --ship",
        "preversion": "node ./tools/pre-version.js && npm run format && git add .",
        "format": "prettier --write {src,config,tools}/**/*.{ts,tsx,js,json,css,scss} *.{json,js}",
        "stylelint:scss": "stylelint --syntax scss **/*.scss",
        "lint": "eslint src/* --ext js,ts,tsx",
        "lint:fix": "npm run lint -- --fix",
        "serve": "gulp bundle --custom-serve --max_old_space_size=4096 && fast-serve"
    },
    "dependencies": {
        "@microsoft/decorators": "1.15.2",
        "@microsoft/sp-adaptive-card-extension-base": "1.15.2",
