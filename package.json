        "@types/react": "16.9.51",
        "@types/react-dom": "16.9.8",
        "@types/react-sortable-tree": "0.3.15",
        "@types/webpack-env": "1.15.2",
        "ajv": "6.12.5",
        "eslint-config-prettier": "8.5.0",
